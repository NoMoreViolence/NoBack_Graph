<!DOCTYPE html>
<html>

<head>
    <title>Graph_Game</title>
    <!-- CSS and Javascript -->
    <meta name="viewport" content="width=device-width">
    <link rel='stylesheet' href='/stylesheets/rank.css' />
</head>

<body>
    <div class="container">
        <div class="header">
            <div class="object">
                <a href="/">Graph</a>
            </div>
            <div class="menu">
                <ol>   
                    <li><a href="/start">Game</a></li>
                    <li><a href="/rank">Ranking</a></li>
                    <li><a href="/donation">Donation</a></li>
                    <li><a href="/more">More</a></li>
                </ol>
            </div>
        </div>
        <div class="middle">
            <table>
                <tbody>
                    <% let num = 1; %>
                    <% data.forEach((item, index) => { %>
                        <tr>
                            <td class="rank_num">
                                <%= num %>
                            </td>
                            <td class="rank_username">
                                <%= item.username %>
                            </td>
                            <td class="rank_money">
                                <%= item.money %>
                            </td>
                        </tr>
                    <% num += 1; %>
                    <% }) %>
                </tbody>
            </table>
        </div>
    </div>
</body>

</html>